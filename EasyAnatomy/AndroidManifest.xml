<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.quesucede.gameoflife" android:versionCode="1"
          android:versionName="1.0">
    <application android:icon="@drawable/body_icon" android:label="@string/app_name">
        <activity android:name="com.example.Simp.MainActivity" android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.Simp.about.AboutActivity"
                  android:label="@string/about_title"
                  android:theme="@android:style/Theme.Dialog">
        </activity>
        <activity android:name="com.example.Simp.HumanActivity"
                  android:label="@string/learn_label" />
        <activity android:name="com.example.Simp.human_activity.BrainActivity"
                  android:theme="@android:style/Theme.Dialog"
                  android:label="@string/brain_title"/>
        <activity android:name="com.example.Simp.human_activity.LungsActivity"
                  android:theme="@android:style/Theme.Dialog"
                  android:label="@string/lungs_title"/>
        <activity android:name="com.example.Simp.human_activity.HeartActivity"
                  android:theme="@android:style/Theme.Dialog"
                  android:label="@string/heart_title"/>
        <activity android:name="com.example.Simp.test.ChooseTestActivity"/>
        <activity android:name="com.example.Simp.test.StartTestActivity"/>
        <activity android:name="com.example.Simp.results.ResultsActivity" />
    </application>
</manifest>
